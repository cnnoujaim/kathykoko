{
  "name": "kathykoko",
  "version": "1.0.0",
  "description": "SMS-native AI Chief of Staff - Project Kathy Koko",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "ts-node scripts/migrate.ts",
    "seed": "ts-node -r tsconfig-paths/register scripts/seed-goals.ts",
    "seed:accounts": "ts-node -r tsconfig-paths/register scripts/seed-accounts.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "ai",
    "assistant",
    "sms",
    "twilio",
    "anthropic",
    "claude"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@sentry/node": "^7.90.0",
    "bull": "^4.12.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.11.3",
    "pgvector": "^0.1.5",
    "twilio": "^4.20.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
